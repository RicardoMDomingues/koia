{
   "version": "0.2.0",
   "configurations": [
   
     {
       "name": "chrome",
       "type": "chrome",
       "request": "launch",
       "url": "http://localhost:4200",
       "sourceMaps": true,
       "trace": true,
       "webRoot": "${workspaceRoot}",
       "userDataDir": "${workspaceRoot}/.vscode/chrome"
     },   
     {
       "name": "Debug tests",
       "type": "chrome",
       "request": "attach",
       "port": 9222,
       "sourceMaps": true,
       "webRoot": "${workspaceRoot}"
     }   
   ]
 }